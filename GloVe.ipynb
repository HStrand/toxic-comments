    "for label in pipe.class_labels:\n",
    "    pipe.log(\"Fitting submission classifier for \" + label)\n",
    "    # y_train = np.array(pipe.train[label])\n",
    "    X_train, X_test, y_train, y_test = train_test_split(pipe.train_features, list(pipe.train[label]), test_size=0.2, random_state=40)\n",
    "    xgb.fit(X_train, y_train, eval_metric=\"auc\", early_stopping_rounds=10, eval_set=[(X_test, y_test)])\n",
    "    pipe.predictions[\"XGBoost\"][label] = xgb.predict_proba(pipe.test_features)"
   ]
